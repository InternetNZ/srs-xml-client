<NZSRSRequest VerMajor="{{ version_major }}" VerMinor="{{ version_minor }}">
  <RunLogCreate ActionId="{{ action_id }}">
    <FirstRunDate Year="{{ first_run.year }}" Month="{{ first_run.month }}" Day="{{ first_run.day }}" Hour="{{ first_run.hour }}" Minute="{{ first_run.minute }}" Second="{{ first_run.second }}" {% if first_run.time_zone %} TimeZoneOffset="{{ first_run.time_zone }}" {% endif %}/>
    <RunLog ProcessName="{{ process_name }}"
      {% if parameters %} Parameters="{{ parameters }}" {% endif %}
      {% if success %} ActionStatus="Successful" {% elif failure %} ActionStatus="Failure" {% endif %}
      {% if control %} Control="{{ control }}" {% endif %}>
      <RunLogTimeStamp Year="{{ logged.year }}" Month="{{ logged.month }}" Day="{{ logged.day }}" Hour="{{ logged.hour }}" Minute="{{ logged.minute }}" Second="{{ logged.second }}" {% if logged.time_zone %} TimeZoneOffset="{{ logged.time_zone }}" {% endif %} />
      {% if details %} <RunLogDetails>{{ details }}</RunLogDetails> {% endif %}
    </RunLog>
  </RunLogCreate>
</NZSRSRequest>
